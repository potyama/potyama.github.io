hemeConfig.nav || []\n    },\n\n    nav () {\n      const { locales } = this.$site\n      if (locales && Object.keys(locales).length > 1) {\n        const currentLink = this.$page.path\n        const routes = this.$router.options.routes\n        const themeLocales = this.$site.themeConfig.locales || {}\n        const languageDropdown = {\n          text: this.$themeLocaleConfig.selectText || 'Languages',\n          ariaLabel: this.$themeLocaleConfig.ariaLabel || 'Select language',\n          items: Object.keys(locales).map(path => {\n            const locale = locales[path]\n            const text = themeLocales[path] && themeLocales[path].label || locale.lang\n            let link\n            // Stay on the current page\n            if (locale.lang === this.$lang) {\n              link = currentLink\n            } else {\n              // Try to stay on the same page\n              link = currentLink.replace(this.$localeConfig.path, path)\n              // fallback to homepage\n              if (!routes.some(route => route.path === link)) {\n                link = path\n              }\n            }\n            return { text, link }\n          })\n        }\n        return [...this.userNav, languageDropdown]\n      }\n      return this.userNav\n    },\n\n    userLinks () {\n      return (this.nav || []).map(link => {\n        return Object.assign(resolveNavLinkItem(link), {\n          items: (link.items || []).map(resolveNavLinkItem)\n        })\n      })\n    },\n\n    repoLink () {\n      const { repo } = this.$site.themeConfig\n      if (repo) {\n        return /^https?:/.test(repo)\n          ? repo\n          : `https://github.com/${repo}`\n      }\n      return null\n    },\n\n    repoLabel () {\n      if (!this.repoLink) return\n      if (this.$site.themeConfig.repoLabel) {\n        return this.$site.themeConfig.repoLabel\n      }\n\n      const repoHost = this.repoLink.match(/^https?:\\/\\/[^/]+/)[0]\n      const platforms = ['GitHub', 'GitLab', 'Bitbucket']\n      for (let i = 0; i < platforms.length; i++) {\n        const platform = platforms[i]\n        if (new RegExp(platform, 'i').test(repoHost)) {\n          return platform\n        }\n      }\n\n      return 'Source'\n    }\n  }\n}\n"],"mappings":"AACA,OAAOA,YAAY,MAAM,oCAAoC;AAC7D,SAASC,kBAAkB,QAAQ,SAAS;AAC5C,OAAOC,OAAO,MAAM,+BAA+B;AAEnD,eAAe;EACbC,IAAI,EAAE,UAAU;EAEhBC,UAAU,EAAE;IACVF,OAAO;IACPF;EACF,CAAC;EAEDK,QAAQ,EAAE;IACRC,OAAOA,CAAA,EAAI;MACT,OAAO,IAAI,CAACC,kBAAkB,CAACC,GAAG,IAAI,IAAI,CAACC,KAAK,CAACC,WAAW,CAACF,GAAG,IAAI,EAAE;IACxE,CAAC;IAEDA,GAAGA,CAAA,EAAI;MACL,MAAM;QAAEG;MAAQ,CAAC,GAAG,IAAI,CAACF,KAAK;MAC9B,IAAIE,OAAO,IAAIC,MAAM,CAACC,IAAI,CAACF,OAAO,CAAC,CAACG,MAAM,GAAG,CAAC,EAAE;QAC9C,MAAMC,WAAW,GAAG,IAAI,CAACC,KAAK,CAACC,IAAI;QACnC,MAAMC,MAAM,GAAG,IAAI,CAACC,OAAO,CAACC,OAAO,CAACF,MAAM;QAC1C,MAAMG,YAAY,GAAG,IAAI,CAACZ,KAAK,CAACC,WAAW,CAACC,OAAO,IAAI,CAAC,CAAC;QACzD,MAAMW,gBAAgB,GAAG;UACvBC,IAAI,EAAE,IAAI,CAAChB,kBAAkB,CAACiB,UAAU,IAAI,WAAW;UACvDC,SAAS,EAAE,IAAI,CAAClB,kBAAkB,CAACkB,SAAS,IAAI,iBAAiB;UACjEC,KAAK,EAAEd,MAAM,CAACC,IAAI,CAACF,OAAO,CAAC,CAACgB,GAAG,CAACV,IAAI,IAAI;YACtC,MAAMW,MAAM,GAAGjB,OAAO,CAACM,IAAI,CAAC;YAC5B,MAAMM,IAAI,GAAGF,YAAY,CAACJ,IAAI,CAAC,IAAII,YAAY,CAACJ,IAAI,CAAC,CAACY,KAAK,IAAID,MAAM,CAACE,IAAI;YAC1E,IAAIC,IAAI;YACR;YACA,IAAIH,MAAM,CAACE,IAAI,KAAK,IAAI,CAACE,KAAK,EAAE;cAC9BD,IAAI,GAAGhB,WAAW;YACpB,CAAC,MAAM;cACL;cACAgB,IAAI,GAAGhB,WAAW,CAACkB,OAAO,CAAC,IAAI,CAACC,aAAa,CAACjB,IAAI,EAAEA,IAAI,CAAC;cACzD;cACA,IAAI,CAACC,MAAM,CAACiB,IAAI,CAACC,KAAK,IAAIA,KAAK,CAACnB,IAAI,KAAKc,IAAI,CAAC,EAAE;gBAC9CA,IAAI,GAAGd,IAAI;cACb;YACF;YACA,OAAO;cAAEM,IAAI;cAAEQ;YAAK,CAAC;UACvB,CAAC;QACH,CAAC;QACD,OAAO,CAAC,GAAG,IAAI,CAACzB,OAAO,EAAEgB,gBAAgB,CAAC;MAC5C;MACA,OAAO,IAAI,CAAChB,OAAO;IACrB,CAAC;IAED+B,SAASA,CAAA,EAAI;MACX,OAAO,CAAC,IAAI,CAAC7B,GAAG,IAAI,EAAE,EAAEmB,GAAG,CAACI,IAAI,IAAI;QAClC,OAAOnB,MAAM,CAAC0B,MAAM,CAACrC,kBAAkB,CAAC8B,IAAI,CAAC,EAAE;UAC7CL,KAAK,EAAE,CAACK,IAAI,CAACL,KAAK,IAAI,EAAE,EAAEC,GAAG,CAAC1B,kBAAkB;QAClD,CAAC,CAA